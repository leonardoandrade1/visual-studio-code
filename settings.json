 {
     // Place your settings in this file to overwrite default and user settings. {
     /*Hasklig Medium, Aurulent Sans Mono, Courier Prime Code, Office Code Pro D, Hasklig, Droid Sans Mono, DejaVu Sans Mono,
     Andale Mono, courier, Source Code Pro, Roboto Mono, Hack, Code New Roman, Fira Mono, Inconsolata-g, Kawkab Mono, Liberation Mono,
     Mensch, Meslo, Metrickal, Luxi, Nanum Gotic Coding, Oxygen Mono, Panic Sans, Sudo  */
    // "editor.fontFamily": "'Operator Mono Book', 'Roboto Mono', monospace, 'Droid Sans Mono'",
    //"editor.fontFamily": "'Hasklig Medium'",
    "editor.fontFamily": "'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 14,
    "terminal.integrated.fontSize": 14,
    "editor.minimap.enabled": true,
    "editor.minimap.showSlider": "always",
    "editor.fontLigatures": true,
    "editor.glyphMargin": false,
    "window.openFilesInNewWindow": "on",
    "explorer.openEditors.visible": 0,
    "editor.mouseWheelZoom": true,
    "editor.detectIndentation": true,
    "editor.tabCompletion": true,
    "editor.lineHeight": 30,
    "editor.autoClosingBrackets": true,
    "editor.formatOnType": true,
    "editor.renderIndentGuides": true,
    "workbench.editor.enablePreview": false,
    "typescript.useCodeSnippetsOnMethodSuggest": true,
    "editor.formatOnSave": false,
    "editor.wordWrap": "on",
    "editor.cursorBlinking": "phase",
    "window.zoomLevel": 0,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.formatOnPaste": true,
    "files.autoSave": "off",
    "files.associations": {
        "*.cjsx": "javascriptreact"
    },
    "material-icon-theme.folders.theme": "specific",
    "extensions.ignoreRecommendations": false,
    "workbench.iconTheme": "material-icon-theme",
    "material-icon-theme.activeIconPack": "angular",
    "editor.showFoldingControls": "mouseover",
    "editor.tabSize": 4,
    "files.trimTrailingWhitespace": true,
    "editor.snippetSuggestions": "top",
    "path-intellisense.extensionOnImport": true,
    "workbench.colorTheme": "Winter is Coming (Dark)"
}


